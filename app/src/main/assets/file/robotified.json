{
  "$jason": {
    "body": {
      "style": {
        "background": "#9a79ba",
        "border": "none"
      },
      "sections": [
        {
          "items": [
            {
              "style": {
                "padding_top": 48,
                "padding_right": 128,
                "padding_bottom": 48,
                "padding_left": 128,
                "color": "#ffffff",
                "size": 48,
                "align": "center",
                "border": "none rgb(255, 255, 255)"
              },
              "ref": "h1",
              "type": "label",
              "text": "How can I help you?"
            },
            {
              "style": {
                "padding_top": 16,
                "padding_right": 320,
                "padding_bottom": 16,
                "padding_left": 320,
                "size": 24,
                "align": "center",
                "color": "#fff502"
              },
              "type": "vertical",
              "ref": "footer",
              "components": [
                {
                  "type": "spinner",
                  "name": "Language",
                  "data": "English|Arabic",
                  "href_en": {
                    "url": "https://robotics.provensolution.com/custom/applications/jasonette/639"
                  },
                  "href_ar": {
                    "url": "https://robotics.provensolution.com/custom/applications/jasonette/631"
                  },
                  "style": {
                    "width": "150",
                    "height": "60"
                  }
                },
                {
                  "style": {
                    "size": 24,
                    "color": "#fff502",
                    "align": "center"
                  },
                  "type": "label",
                  "text": "Speak instead of tapping!"
                },
                {
                  "type": "speak",
                  "text": "The text the ROBOT should speak, Greetings"
                },
                {
                  "type": "button",
                  "text": "Next",
                  "robotified": "true",
                  "conversationId": "122",
                  "href": {
                    "url": "https://devdtoo.github.io/SDUI-Jsons/test.json"
                  },
                  "style": {
                    "width": "100",
                    "height": "60",
                    "background": "#000000",
                    "color": "#ffffff",
                    "font": "HelveticaNeue",
                    "size": "12",
                    "corner_radius": "25",
                    "align": "center"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  }
}

